package com.example.tempatemetod;

public abstract class BankTemplateMethod {
	public void takeNumber() {
		System.out.println("");
	}

	public abstract void transact();

	public void evaluate() {
		System.out.println("");
	}

	public final void process() {
		this.takeNumber();

		this.transact();

		this.evaluate();
	}

}
